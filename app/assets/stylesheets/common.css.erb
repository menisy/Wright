.alert-wrapper{
  position: fixed;
  width: 99%;
  left: 0px;
  top: 50px;
}
.dropdown-menu li a{
  width: 87px;
  display: inline-block !important;
  margin-left: 5px;
}
.dropdown-menu li{
  width: 120px !important;
}
.dropdown-menu li span{
  margin-left: 5px;
}
.dropdown{
  padding-top: 10px;
}
a:hover{
  text-decoration: none !important;
}
.seventy{
  width: 87% !important;
}
.inst{
  width: 400px;
}
.upper-box{
  direction: ltr !important;
}
.instructions{
  color: #fff;
}
.right-box{
  height: 400px;
  margin-top: -400px;
}
.slider-vertical{
  margin: 9px;
}
.white{
  color: white;
}
ul.mid{
  width: 38%;
  text-align: center;
  margin: 0 auto;
  height: 20px;
  margin-top: -15px;
}
.mid li{
  color: #fff;
  font-family: fantasy;
}
body{
  background-image: url(<%= asset_path 'letters.jpg' %>);
  background-size: 150px;
}
#main-container{
  background-color: #00B9BD;
  height: 100%;
  margin-top: 20px;
  padding-bottom: 15px;
}
nav .container{
  width: 70%;
  padding-top: 7px;
}
.logo{
  width: 80px;
  position: absolute;
  top: 5px;
}
.logo-inv{
  display: inline-block;
  width: 70px;
  background-image: url(<%= asset_path 'logo-inv.png' %>);
  height: 50px;
  position: absolute;
  top: 1px;
  background-size: 70px;
}
.navbar{
  height: 65px;
}
.navbar .btn{
  background-color: #00B9BD;
  color: white;
}
.navbar .btn:hover{
  color: black;
}
.user-prev{
  padding: 10px;
  border: 1px solid #002b32;
  border-radius: 5px;
  margin-bottom: 5px;
}
.user-prev div{
  display: inline-block;
}
.user-prev .pic{
  border: 1px solid #eee;
}
.user-prev .score{
  padding: 10px;
}
